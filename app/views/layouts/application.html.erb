
<!DOCTYPE HTML>
<html>
<head>
  <title>Kampus Korner</title>

  <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

  <%= stylesheet_link_tag 'style' %>
  <%= stylesheet_link_tag 'slider-styles' %>
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
  <%= javascript_include_tag 'jquery.min' %>
  <%= javascript_include_tag 'slider' %>
  <%= javascript_include_tag 'jquery-ui.min' %>
  <%= javascript_include_tag 'jquery.slimscroll.min' %>


</head>
<body>
<div class="body_header" >
  <div class="top-head">
    <div class="welcome">Welcome To <span>Kampus Korner</span></div>
    <div class="top-nav">
      <ul>

        <li class="active"><%=link_to "Home" %></li>
        <li ><%=link_to "Gallery" , gallaries_path %></li>
        <li><%=link_to "Contact" , opinions_index_path %></li>
        <li><%=link_to "Login" , admin_dashboard_path %></li>
      </ul>
    </div>
    <div class="clear"> </div>
  </div>
</div>
<div class="body_main">
<div class="wrap">

<%= render 'home/header' %>
  <%= yield %>
</div>
  </div>
<%= render 'home/footer'%>
<script>
    $('#slider').coinslider();

</script>

</body>
</html>
